{
    "name": "ilma/ecosystem",
    "type": "library",
    "description": "Ilma ecosystem library",
    "license": "MIT",
    "authors": [
        {
            "name": "SMSkin",
            "email": "serg@msoinvest.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "laravel/framework": "^7.0",
        "tymon/jwt-auth": "^1.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "laracademy/generators": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Ilma\\Ecosystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ilma\\Ecosystem\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Ilma\\Ecosystem\\ServiceProvider"
            ]
        }
    }
}
